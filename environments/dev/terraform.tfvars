# Variable values for the development environment

# AWS Configuration
aws_region = "us-east-1"
aws_instance_type = "t2.micro"
aws_key_name = "your-key-name"

# GCP Configuration
gcp_project_id = "your-gcp-project-id"
gcp_region = "us-central1"
gcp_instance_type = "f1-micro"

# Azure Configuration
azure_region = "East US"
azure_vm_size = "Standard_B1s"
azure_admin_username = "azureuser"
azure_admin_password = "your-password"

# DNS Configuration
dns_zone_name = "example.com"
dns_failover_enabled = true

# Storage Configuration
aws_s3_bucket_name = "your-aws-s3-bucket"
gcp_gcs_bucket_name = "your-gcp-gcs-bucket"
azure_blob_container_name = "your-azure-blob-container"

# Additional Variables
environment = "dev"
enable_logging = true
enable_monitoring = true